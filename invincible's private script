-- Invincibles Priv Script - Double-Checked & Fixed for Elerium v2 (Muscle Legends, Feb 2026)
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library", true))()

local window = library:AddWindow("Invincibles Priv Script", {
    main_color = Color3.fromRGB(50, 50, 50),
    min_size = Vector2.new(470, 470),
    can_resize = false,
})

-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local lp = Players.LocalPlayer
local muscleEvent = lp:WaitForChild("muscleEvent", 10)

-- State
local states = {
    AutoPushups = false,
    AutoWeight = false,
    AutoHandstands = false,
    FastPunch = false,
    FastStrength = false,
    InfiniteJump = false,
    KillAura = false,
    AutoKillSelected = false,
    AutoRebirth = false,
    AutoRebirthTarget = false,
    AutoSize2 = false,
    AutoTPKing = false,
    HidePets = false,
}

local targetRebirths = 1
local selectedTarget = nil

getgenv().rockFarmActive = false
getgenv().selectedRockDur = 0

-- Helpers
local function safeEquip(toolName)
    task.spawn(function()
        local char = lp.Character
        if not char or not char:FindFirstChild("Humanoid") then return end
        local tool = lp.Backpack:FindFirstChild(toolName) or char:FindFirstChild(toolName)
        if tool and tool.Parent ~= char then
            char.Humanoid:EquipTool(tool)
        end
    end)
end

local function tpTo(pos)
    local hrp = lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")
    if hrp then
        hrp.CFrame = CFrame.new(pos)
    end
end

-- Auto Rep
local function startAutoRep(toolName, key)
    task.spawn(function()
        while states[key] do
            pcall(function()
                local char = lp.Character
                if char and char.Humanoid and char.Humanoid.Health > 0 then
                    safeEquip(toolName)
                    if muscleEvent then muscleEvent:FireServer("rep") end
                end
            end)
            task.wait(0.08 + math.random()/100)
        end
    end)
end

-- Fast Strength
local fastStrengthConn
local function toggleFastStrength(enabled)
    states.FastStrength = enabled
    if fastStrengthConn then fastStrengthConn:Disconnect() fastStrengthConn = nil end
    if enabled then
        fastStrengthConn = RunService.Heartbeat:Connect(function()
            pcall(function()
                if muscleEvent then
                    muscleEvent:FireServer("rep")
                    muscleEvent:FireServer("punch", "rightHand")
                    muscleEvent:FireServer("punch", "leftHand")
                end
                local char = lp.Character
                if char then
                    local tool = char:FindFirstChildWhichIsA("Tool")
                    if tool then tool:Activate() end
                end
            end)
        end)
    end
end

-- Fast Punch
local function toggleFastPunch(enabled)
    states.FastPunch = enabled
    if enabled then
        task.spawn(function()
            while states.FastPunch do
                pcall(function()
                    if muscleEvent then
                        muscleEvent:FireServer("punch", "rightHand")
                        muscleEvent:FireServer("punch", "leftHand")
                    end
                    safeEquip("Punch")
                    local char = lp.Character
                    if char then
                        local punch = char:FindFirstChild("Punch")
                        if punch then
                            local atk = punch:FindFirstChild("attackTime")
                            if atk and atk:IsA("NumberValue") then atk.Value = 0.01 end
                            punch:Activate()
                        end
                    end
                end)
                task.wait(0.06)
            end
        end)
    end
end

-- Infinite Jump
local infJumpConn
local function toggleInfJump(enabled)
    states.InfiniteJump = enabled
    if infJumpConn then infJumpConn:Disconnect() infJumpConn = nil end
    if enabled then
        infJumpConn = UserInputService.JumpRequest:Connect(function()
            local hum = lp.Character and lp.Character:FindFirstChildOfClass("Humanoid")
            if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
        end)
    end
end

-- Rock Farm
local rockDurMap = {
    [0] = "Tiny Island",
    [100] = "Starter Island",
    [5000] = "Legend Beach",
    [150000] = "Frost Gym",
    [400000] = "Mythical Gym",
    [750000] = "Eternal Gym",
    [1000000] = "Legend Gym",
    [5000000] = "Muscle King",
    [10000000] = "Ancient Jungle",
}

task.spawn(function()
    while true do
        if not getgenv().rockFarmActive or getgenv().selectedRockDur == 0 then
            task.wait(0.5) continue
        end
        pcall(function()
            local char = lp.Character
            if not char or not char:FindFirstChild("Humanoid") or char.Humanoid.Health <= 0 then
                task.wait(1) return
            end
            local durVal = lp:FindFirstChild("Durability")
            if not durVal or durVal.Value < getgenv().selectedRockDur then
                task.wait(0.4) return
            end
            safeEquip("Punch")
            for _, obj in pairs(Workspace.machinesFolder:GetDescendants()) do
                if obj.Name == "neededDurability" and obj:IsA("IntValue") and obj.Value == getgenv().selectedRockDur then
                    local rock = obj.Parent:FindFirstChild("Rock")
                    if rock and rock:IsA("BasePart") then
                        if char:FindFirstChild("RightHand") then
                            firetouchinterest(rock, char.RightHand, 0)
                            task.wait(0.015 + math.random()/50)
                            firetouchinterest(rock, char.RightHand, 1)
                        end
                        if char:FindFirstChild("LeftHand") then
                            firetouchinterest(rock, char.LeftHand, 0)
                            task.wait(0.015 + math.random()/50)
                            firetouchinterest(rock, char.LeftHand, 1)
                        end
                    end
                end
            end
            if muscleEvent then
                muscleEvent:FireServer("punch", "leftHand")
                muscleEvent:FireServer("punch", "rightHand")
            end
        end)
        task.wait(0.15 + math.random()/20)
    end
end)

local function toggleRockFarm(dur, enabled)
    if enabled then
        getgenv().rockFarmActive = false
        task.wait(0.1)
        getgenv().selectedRockDur = dur
        getgenv().rockFarmActive = true
        print("[Rock Farm] Activated → " .. (rockDurMap[dur] or dur))
    else
        if getgenv().selectedRockDur == dur then
            getgenv().rockFarmActive = false
            getgenv().selectedRockDur = 0
            print("[Rock Farm] Stopped")
        end
    end
end

-- Kill Player
local function killPlayer(target)
    pcall(function()
        local char = lp.Character
        if not char or not char:FindFirstChild("LeftHand") then return end
        local tChar = target.Character
        if tChar and tChar:FindFirstChild("HumanoidRootPart") and tChar.Humanoid and tChar.Humanoid.Health > 0 then
            firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 0)
            task.wait(0.03)
            firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 1)
            safeEquip("Punch")
            if muscleEvent then
                muscleEvent:FireServer("punch", "leftHand")
                muscleEvent:FireServer("punch", "rightHand")
            end
        end
    end)
end

-- UI Tabs & Folders
local mainTab = window:AddTab("Main")
local farmingTab = window:AddTab("Fast Farming")
local rebirthTab = window:AddTab("Rebirth")
local teleportTab = window:AddTab("Teleport")
local killTab = window:AddTab("Kill")
local miscTab = window:AddTab("Misc")

local toolsFolder = mainTab:AddFolder("Tools")
local rockFarmFolder = mainTab:AddFolder("Rock Farm")
local killFolder = killTab:AddFolder("Auto Kill")

-- Main Tools
toolsFolder:AddSwitch("Auto Pushups", function(v)
    states.AutoPushups = v
    if v then startAutoRep("Pushups", "AutoPushups") end
end)

toolsFolder:AddSwitch("Auto Weight", function(v)
    states.AutoWeight = v
    if v then startAutoRep("Weight", "AutoWeight") end
end)

toolsFolder:AddSwitch("Auto Handstands", function(v)
    states.AutoHandstands = v
    if v then startAutoRep("Handstands", "AutoHandstands") end
end)

toolsFolder:AddSwitch("Auto Punch (Fast)", toggleFastPunch)

toolsFolder:AddButton("Anti Lag (Boost FPS)", function()
    local Lighting = game:GetService("Lighting")
    Lighting.GlobalShadows = false
    Lighting.Brightness = 1
    Lighting.ClockTime = 12
    Lighting.FogEnd = 9999
    Lighting.FogStart = 0
    for _, v in pairs(Workspace:GetDescendants()) do
        if v:IsA("ParticleEmitter") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") or
           v:IsA("BloomEffect") or v:IsA("BlurEffect") or v:IsA("DepthOfFieldEffect") or v:IsA("SunRaysEffect") then
            v.Enabled = false
        end
    end
    settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
    print("Anti-Lag activated → check FPS (Shift+F5)")
end)

toolsFolder:AddButton("Free AutoLift Gamepass", function()
    pcall(function()
        local gpFolder = ReplicatedStorage:FindFirstChild("gamepassIds")
        if gpFolder then
            local owned = lp:FindFirstChild("ownedGamepasses") or Instance.new("Folder")
            owned.Name = "ownedGamepasses"
            owned.Parent = lp
            for _, gp in pairs(gpFolder:GetChildren()) do
                local val = Instance.new("IntValue")
                val.Name = gp.Name
                val.Value = gp.Value
                val.Parent = owned
            end
            print("Gamepasses spoofed (may require relog)")
        else
            print("gamepassIds not found")
        end
    end)
end)

-- Teleport Tab
teleportTab:AddButton("To Spawn / Safe Area", function() tpTo(Vector3.new(-6, 5, -196)) end)
teleportTab:AddButton("To Secret Area", function() tpTo(Vector3.new(1947, 2, 6191)) end)
teleportTab:AddButton("To Tiny Area", function() tpTo(Vector3.new(-39, 6, 1886)) end)
teleportTab:AddButton("To Frozen Area", function() tpTo(Vector3.new(-2623, 6, -409)) end)
teleportTab:AddButton("To Mythical Area", function() tpTo(Vector3.new(2251, 6, 1073)) end)
teleportTab:AddButton("To Inferno Area", function() tpTo(Vector3.new(-6759, 6, -1285)) end)
teleportTab:AddButton("To Legend Area", function() tpTo(Vector3.new(4603, 990, -3898)) end)
teleportTab:AddButton("To Muscle King Area", function() tpTo(Vector3.new(-8626, 16, -5730)) end)
teleportTab:AddButton("To Jungle Area", function() tpTo(Vector3.new(-8642, 6, 2381)) end)
teleportTab:AddButton("To Lava Brawl", function() tpTo(Vector3.new(4471, 119, -8836)) end)
teleportTab:AddButton("To Desert Brawl", function() tpTo(Vector3.new(960, 17, -7398)) end)
teleportTab:AddButton("To Beach Brawl", function() tpTo(Vector3.new(-1849, 20, -6335)) end)

-- Rock Farm
rockFarmFolder:AddSwitch("Farm Tiny Island Rock (0)", function(v) toggleRockFarm(0, v) end)
rockFarmFolder:AddSwitch("Farm Starter Island Rock (100)", function(v) toggleRockFarm(100, v) end)
rockFarmFolder:AddSwitch("Farm Legend Beach Rock (5000)", function(v) toggleRockFarm(5000, v) end)
rockFarmFolder:AddSwitch("Farm Frost Gym Rock (150k)", function(v) toggleRockFarm(150000, v) end)
rockFarmFolder:AddSwitch("Farm Mythical Gym Rock (400k)", function(v) toggleRockFarm(400000, v) end)
rockFarmFolder:AddSwitch("Farm Eternal Gym Rock (750k)", function(v) toggleRockFarm(750000, v) end)
rockFarmFolder:AddSwitch("Farm Legend Gym Rock (1M)", function(v) toggleRockFarm(1000000, v) end)
rockFarmFolder:AddSwitch("Farm Muscle King Gym Rock (5M)", function(v) toggleRockFarm(5000000, v) end)
rockFarmFolder:AddSwitch("Farm Ancient Jungle Rock (10M)", function(v) toggleRockFarm(10000000, v) end)
rockFarmFolder:AddButton("STOP All Rock Farming", function()
    getgenv().rockFarmActive = false
    getgenv().selectedRockDur = 0
    print("[Rock Farm] All farming stopped")
end)

-- Kill Tab
killFolder:AddSwitch("Kill Aura (All)", function(v)
    states.KillAura = v
    if v then
        task.spawn(function()
            while states.KillAura do
                for _, plr in Players:GetPlayers() do
                    if plr ~= lp and plr.Character then
                        killPlayer(plr)
                    end
                end
                task.wait(0.12)
            end
        end)
    end
end)

local playerDropdown = killFolder:AddDropdown("Select Target Player", function(val)
    selectedTarget = val:match("^([^|]+)") or val
end)

killFolder:AddSwitch("Auto Kill Selected", function(v)
    states.AutoKillSelected = v
    if v and selectedTarget then
        task.spawn(function()
            while states.AutoKillSelected do
                local plr = Players:FindFirstChild(selectedTarget)
                if plr then killPlayer(plr) end
                task.wait(0.12)
            end
        end)
    end
end)

task.spawn(function()
    while true do
        local opts = {}
        for _, plr in Players:GetPlayers() do
            if plr ~= lp then
                table.insert(opts, plr.Name .. " | " .. (plr.DisplayName or plr.Name))
            end
        end
        playerDropdown:SetValues(opts)
        task.wait(6)
    end
end)

-- Fast Farming Tab
farmingTab:AddSwitch("Fast Grind / Strength", toggleFastStrength)
farmingTab:AddSwitch("Hide Pets", function(v)
    states.HidePets = v
    pcall(function()
        local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
        if rEvents and rEvents:FindFirstChild("showPetsEvent") then
            rEvents.showPetsEvent:FireServer(v and "hidePets" or "showPets")
        end
    end)
end)

-- Rebirth Tab
rebirthTab:AddSwitch("Auto Rebirth (Infinite)", function(v)
    states.AutoRebirth = v
    if v then
        task.spawn(function()
            while states.AutoRebirth do
                pcall(function()
                    local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
                    if rEvents and rEvents:FindFirstChild("rebirthRemote") then
                        rEvents.rebirthRemote:InvokeServer("rebirthRequest")
                    end
                end)
                task.wait(0.12)
            end
        end)
    end
end)

rebirthTab:AddInput("Target Rebirths", function(value)
    local num = tonumber(value)
    targetRebirths = num and num > 0 and num or 1
    print("Target rebirths set to: " .. targetRebirths)
end)

rebirthTab:AddSwitch("Auto Rebirth to Target", function(v)
    states.AutoRebirthTarget = v
    if v then
        task.spawn(function()
            while states.AutoRebirthTarget do
                pcall(function()
                    if lp.leaderstats and lp.leaderstats:FindFirstChild("Rebirths") then
                        if lp.leaderstats.Rebirths.Value >= targetRebirths then
                            states.AutoRebirthTarget = false
                            print("Target rebirth reached → stopped")
                            return
                        end
                    end
                    local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
                    if rEvents and rEvents:FindFirstChild("rebirthRemote") then
                        rEvents.rebirthRemote:InvokeServer("rebirthRequest")
                    end
                end)
                task.wait(0.14)
            end
        end)
    end
end)

rebirthTab:AddSwitch("Auto Size 2", function(v)
    states.AutoSize2 = v
    if v then
        task.spawn(function()
            while states.AutoSize2 do
                pcall(function()
                    local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
                    if rEvents and rEvents:FindFirstChild("changeSpeedSizeRemote") then
                        rEvents.changeSpeedSizeRemote:InvokeServer("changeSize", 2)
                    end
                end)
                task.wait(0.07)
            end
        end)
    end
end)

rebirthTab:AddSwitch("Auto TP Muscle King", function(v)
    states.AutoTPKing = v
    if v then
        task.spawn(function()
            while states.AutoTPKing do
                pcall(function()
                    tpTo(Vector3.new(-8646, 17, -5738))
                end)
                task.wait(0.15)
            end
        end)
    end
end)

-- Misc Tab
miscTab:AddSwitch("Infinite Jump", toggleInfJump)

lp.CharacterAdded:Connect(function()
    task.wait(2)
    -- Optional: re-apply active features here if needed
end)

print("Invincibles Priv Script – double-checked & ready (Feb 24, 2026)")
