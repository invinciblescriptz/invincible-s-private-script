-- Invincible Lifting Sim / similar game script - CLEANED & FIXED - Feb 2026
-- Loader: https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library"))()
local window = library:AddWindow("Invincible OP Script", {
    main_color = Color3.fromRGB(200, 0, 0),
    min_size = Vector2.new(520, 520),
    can_resize = false,
})

-- Services
local Players           = game:GetService("Players")
local RunService        = game:GetService("RunService")
local UserInputService  = game:GetService("UserInputService")
local VirtualUser       = game:GetService("VirtualUser")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace         = game:GetService("Workspace")
local Lighting          = game:GetService("Lighting")

local LocalPlayer = Players.LocalPlayer
local muscleEvent = LocalPlayer:WaitForChild("muscleEvent", 10)

-- Globals
getgenv().ScriptEnabled = true
local toggles = {
    AutoPushups     = false,
    AutoWeight      = false,
    AutoHandstands  = false,
    FastStrength    = false,
    InfiniteJump    = false,
    AntiAfk         = false,
    KillAura        = false,
    AutoRebirth     = false,
    RockFarmActive  = false,
}

local selectedRockDur = 0
local killAuraConn

-- =============================================================
-- Helpers
-- =============================================================

local function safeTeleport(pos)
    pcall(function()
        local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if root then root.CFrame = CFrame.new(pos) end
    end)
end

local function equipIfExists(name)
    pcall(function()
        local char = LocalPlayer.Character
        if not char or not char:FindFirstChild("Humanoid") then return end
        local tool = LocalPlayer.Backpack:FindFirstChild(name) or char:FindFirstChild(name)
        if tool and tool.Parent ~= char then
            char.Humanoid:EquipTool(tool)
        end
    end)
end

local function startAutoTool(toolName, toggleKey)
    task.spawn(function()
        while toggles[toggleKey] and getgenv().ScriptEnabled do
            pcall(function()
                local char = LocalPlayer.Character
                if not char or char.Humanoid.Health <= 0 then task.wait(1.5) return end
                equipIfExists(toolName)
                if muscleEvent then muscleEvent:FireServer("rep") end
            end)
            task.wait(0.075)
        end
    end)
end

-- =============================================================
-- Feature Toggles / Loops
-- =============================================================

-- Fast Strength & Punch
local fastConn
local function toggleFastStr(enabled)
    toggles.FastStrength = enabled
    if enabled then
        if fastConn then fastConn:Disconnect() end
        fastConn = RunService.Heartbeat:Connect(function()
            pcall(function()
                if muscleEvent then
                    muscleEvent:FireServer("rep")
                    muscleEvent:FireServer("punch", "rightHand")
                    muscleEvent:FireServer("punch", "leftHand")
                end
                local char = LocalPlayer.Character
                local tool = char and char:FindFirstChildWhichIsA("Tool")
                if tool then tool:Activate() end
            end)
        end)
    else
        if fastConn then fastConn:Disconnect() fastConn = nil end
    end
end

-- Infinite Jump
local jumpConn
local function toggleInfJump(enabled)
    toggles.InfiniteJump = enabled
    if enabled then
        jumpConn = UserInputService.JumpRequest:Connect(function()
            pcall(function()
                local hum = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
                if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
            end)
        end)
    else
        if jumpConn then jumpConn:Disconnect() jumpConn = nil end
    end
end

-- Anti-AFK
local afkConn
local function toggleAntiAfk(enabled)
    toggles.AntiAfk = enabled
    if enabled then
        afkConn = LocalPlayer.Idled:Connect(function()
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new())
        end)
    else
        if afkConn then afkConn:Disconnect() afkConn = nil end
    end
end

-- Simple & effective Kill Aura (position under target)
local function tryKill(plr)
    pcall(function()
        local me = LocalPlayer.Character
        local them = plr.Character
        if not (me and them) then return end
        local myRoot = me:FindFirstChild("HumanoidRootPart")
        local tRoot = them:FindFirstChild("HumanoidRootPart")
        if myRoot and tRoot then
            myRoot.CFrame = tRoot.CFrame * CFrame.new(0, -28, 0)
        end
    end)
end

if killAuraConn then killAuraConn:Disconnect() end
killAuraConn = RunService.Heartbeat:Connect(function()
    if not toggles.KillAura then return end
    for _, plr in Players:GetPlayers() do
        if plr ~= LocalPlayer and plr.Character then
            tryKill(plr)
        end
    end
end)

-- Rock Farm Loop
task.spawn(function()
    while getgenv().ScriptEnabled do
        if not toggles.RockFarmActive or selectedRockDur <= 0 then
            task.wait(0.6) continue
        end

        pcall(function()
            local char = LocalPlayer.Character
            if not char or char.Humanoid.Health <= 0 then task.wait(1.2) return end

            local durVal = LocalPlayer:FindFirstChild("Durability")
            if not durVal or durVal.Value < selectedRockDur then task.wait(0.5) return end

            equipIfExists("Punch")

            local touched = false
            for _, obj in Workspace.machinesFolder:GetDescendants() do
                if obj.Name == "neededDurability" and obj:IsA("IntValue") and obj.Value == selectedRockDur then
                    local rock = obj.Parent:FindFirstChild("Rock")
                    if rock and rock:IsA("BasePart") then
                        touched = true
                        local rh = char:FindFirstChild("RightHand")
                        local lh = char:FindFirstChild("LeftHand")
                        if rh then
                            firetouchinterest(rock, rh, 0)
                            task.wait(0.025)
                            firetouchinterest(rock, rh, 1)
                        end
                        if lh then
                            firetouchinterest(rock, lh, 0)
                            task.wait(0.025)
                            firetouchinterest(rock, lh, 1)
                        end
                    end
                end
            end

            if touched and muscleEvent then
                muscleEvent:FireServer("punch", "leftHand")
                muscleEvent:FireServer("punch", "rightHand")
            end
        end)
        task.wait(0.16)
    end
end)

local function setRockFarm(dur, state)
    if state then
        selectedRockDur = dur
        toggles.RockFarmActive = true
    else
        if selectedRockDur == dur then
            toggles.RockFarmActive = false
            selectedRockDur = 0
        end
    end
end

-- =============================================================
-- UI Tabs
-- =============================================================

local mainTab    = window:AddTab("Main")
local farmTab    = window:AddTab("Farm")
local combatTab  = window:AddTab("Combat")
local rebirthTab = window:AddTab("Rebirth")
local tpTab      = window:AddTab("Teleport")
local miscTab    = window:AddTab("Misc")

-- Main - Tools
local toolsFold = mainTab:AddFolder("Auto Tools")
toolsFold:AddSwitch("Auto Pushups",    function(s) toggles.AutoPushups = s    if s then startAutoTool("Pushups",    "AutoPushups")    end end)
toolsFold:AddSwitch("Auto Weight",     function(s) toggles.AutoWeight  = s    if s then startAutoTool("Weight",     "AutoWeight")     end end)
toolsFold:AddSwitch("Auto Handstands", function(s) toggles.AutoHandstands = s if s then startAutoTool("Handstands", "AutoHandstands") end end)
toolsFold:AddSwitch("Fast Strength",   toggleFastStr)

-- Farm - Rocks
local rockFold = farmTab:AddFolder("Rock Farm")
rockFold:AddSwitch("Starter Rock (100)",     function(s) setRockFarm(100,     s) end)
rockFold:AddSwitch("Legend Beach (5000)",    function(s) setRockFarm(5000,    s) end)
rockFold:AddSwitch("Frost Gym (150k)",       function(s) setRockFarm(150000,  s) end)
rockFold:AddSwitch("Mythical (400k)",        function(s) setRockFarm(400000,  s) end)
rockFold:AddSwitch("Eternal (750k)",         function(s) setRockFarm(750000,  s) end)
rockFold:AddSwitch("Legend Gym (1M)",        function(s) setRockFarm(1000000, s) end)
rockFold:AddSwitch("Muscle King (5M)",       function(s) setRockFarm(5000000, s) end)
rockFold:AddButton("STOP All Rock Farm", function()
    toggles.RockFarmActive = false
    selectedRockDur = 0
end)

-- Combat
combatTab:AddSwitch("Kill Aura (Position)", function(s)
    toggles.KillAura = s
end)

-- Rebirth
rebirthTab:AddSwitch("Auto Rebirth (Infinite)", function(s)
    toggles.AutoRebirth = s
    if s then
        task.spawn(function()
            while toggles.AutoRebirth and getgenv().ScriptEnabled do
                pcall(function()
                    local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
                    if rEvents and rEvents:FindFirstChild("rebirthRemote") then
                        rEvents.rebirthRemote:InvokeServer("rebirthRequest")
                    end
                end)
                task.wait(0.11)
            end
        end)
    end
end)

-- Teleport
local tpFold = tpTab:AddFolder("Areas")
tpFold:AddButton("Spawn / Safe",      function() safeTeleport(Vector3.new(-6,    5,   -196))   end)
tpFold:AddButton("Secret Area",       function() safeTeleport(Vector3.new(1947,  2,   6191))   end)
tpFold:AddButton("Muscle King Area",  function() safeTeleport(Vector3.new(-8626, 16,  -5730))  end)
tpFold:AddButton("Legend Area",       function() safeTeleport(Vector3.new(4603,  990, -3898))  end)
tpFold:AddButton("Mythical Area",     function() safeTeleport(Vector3.new(2251,  6,   1073))   end)

-- Misc
miscTab:AddSwitch("Infinite Jump", toggleInfJump)
miscTab:AddSwitch("Anti-AFK",      toggleAntiAfk)

miscTab:AddButton("FPS Boost", function()
    Lighting.GlobalShadows = false
    Lighting.Brightness = 1
    Lighting.FogEnd = 9999
    for _, v in pairs(Workspace:GetDescendants()) do
        if v:IsA("ParticleEmitter") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") or
           v:IsA("BloomEffect") or v:IsA("BlurEffect") or v:IsA("DepthOfFieldEffect") then
            v.Enabled = false
        end
    end
end)

-- Cleanup
window:BindUnload(function()
    getgenv().ScriptEnabled = false
    if fastConn then fastConn:Disconnect() end
    if jumpConn then jumpConn:Disconnect() end
    if afkConn then afkConn:Disconnect() end
    if killAuraConn then killAuraConn:Disconnect() end
    print("Script cleaned up & stopped.")
end)

print("Fixed Invincible script loaded - use responsibly")
