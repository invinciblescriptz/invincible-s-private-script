local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library"))()
local window = library:AddWindow("Invincibles Priv Script", {
    main_color = Color3.fromRGB(0, 255, 0),
    min_size   = Vector2.new(470, 470),
    can_resize = false,
})

-- Services
local Players           = game:GetService("Players")
local LocalPlayer       = Players.LocalPlayer
local Workspace         = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService  = game:GetService("UserInputService")
local RunService        = game:GetService("RunService")

local muscleEvent = LocalPlayer:WaitForChild("muscleEvent", 8)

-- Globals
local whitelist = {}              -- player.Name → true
local targetPlayerName = nil

-- =============================================================
-- Helpers
-- =============================================================
local function startAutoRep(toolName, toggleVar)
    task.spawn(function()
        while _G[toggleVar] do
            pcall(function()
                local char = LocalPlayer.Character
                if not char or not char:FindFirstChild("Humanoid") or char.Humanoid.Health <= 0 then
                    task.wait(1.5) return
                end
                local tool = LocalPlayer.Backpack:FindFirstChild(toolName) or char:FindFirstChild(toolName)
                if tool and tool.Parent ~= char then char.Humanoid:EquipTool(tool) end
                if muscleEvent then muscleEvent:FireServer("rep") end
            end)
            task.wait(0.025)
        end
        pcall(function()
            local char = LocalPlayer.Character
            if char and char:FindFirstChild(toolName) then
                char[toolName].Parent = LocalPlayer.Backpack
            end
        end)
    end)
end

-- Fast Strength (stoppable)
local fastStrengthConn
local function toggleFastStrength(state)
    if state then
        if fastStrengthConn then fastStrengthConn:Disconnect() end
        fastStrengthConn = RunService.Heartbeat:Connect(function()
            pcall(function()
                if muscleEvent then
                    muscleEvent:FireServer("rep")
                    muscleEvent:FireServer("punch", "rightHand")
                    muscleEvent:FireServer("punch", "leftHand")
                end
                local char = LocalPlayer.Character
                if char then
                    local tool = char:FindFirstChildWhichIsA("Tool")
                    if tool then tool:Activate() end
                end
            end)
        end)
        print("[Fast Strength] ON")
    else
        if fastStrengthConn then fastStrengthConn:Disconnect() fastStrengthConn = nil end
        print("[Fast Strength] OFF")
    end
end

-- Simple console-based whitelist display (Elerium v2 has no paragraph/label)
local function updateWhitelistDisplay()
    local list = {}
    for name in pairs(whitelist) do table.insert(list, name) end
    print("Whitelisted players (" .. #list .. "): " .. (next(list) and table.concat(list, ", ") or "none"))
end

-- Kill helper
local function killPlayer(target)
    pcall(function()
        local char = LocalPlayer.Character
        if not char or not char:FindFirstChild("LeftHand") then return end
        local tChar = target.Character
        if tChar and tChar:FindFirstChild("HumanoidRootPart") and tChar.Humanoid and tChar.Humanoid.Health > 0 then
            firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 0)
            task.wait()
            firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 1)
            local punch = LocalPlayer.Backpack:FindFirstChild("Punch") or char:FindFirstChild("Punch")
            if punch then char.Humanoid:EquipTool(punch) end
            if muscleEvent then
                muscleEvent:FireServer("punch", "leftHand")
                muscleEvent:FireServer("punch", "rightHand")
            end
        end
    end)
end

-- =============================================================
-- Tabs & Folders
-- =============================================================
local mainTab     = window:AddTab("Main")
local farmingTab  = window:AddTab("Better Farming")
local rebirthTab  = window:AddTab("Rebirth")
local teleportTab = window:AddTab("Teleport")
local killerTab   = window:AddTab("Killer")
local miscTab     = window:AddTab("Misc")

local toolsFolder = mainTab:AddFolder("Tools")
local killFolder  = killerTab:AddFolder("Kill Controls")

-- =============================================================
-- Main → Tools
-- =============================================================
toolsFolder:AddSwitch("Auto Pushups",    function(s) _G.AutoPushups    = s if s then startAutoRep("Pushups",    "AutoPushups")    end end)
toolsFolder:AddSwitch("Auto Weight",     function(s) _G.AutoWeight     = s if s then startAutoRep("Weight",     "AutoWeight")     end end)
toolsFolder:AddSwitch("Auto Handstands", function(s) _G.AutoHandstands = s if s then startAutoRep("Handstands", "AutoHandstands") end end)

toolsFolder:AddSwitch("Fast Punch", function(state)
    _G.fastPunch = state
    if state then
        task.spawn(function()
            while _G.fastPunch do
                pcall(function()
                    if muscleEvent then
                        muscleEvent:FireServer("punch", "rightHand")
                        muscleEvent:FireServer("punch", "leftHand")
                    end
                    local char = LocalPlayer.Character
                    local punch = char and (char:FindFirstChild("Punch") or LocalPlayer.Backpack:FindFirstChild("Punch"))
                    if punch and punch.Parent ~= char then char.Humanoid:EquipTool(punch) end
                    if punch and punch.Parent == char then punch:Activate() end
                end)
                task.wait(0.04)
            end
        end)
    end
end)

toolsFolder:AddButton("Anti Lag (Boost FPS)", function()
    local Lighting = game:GetService("Lighting")
    Lighting.GlobalShadows = false
    Lighting.Brightness = 1
    Lighting.ClockTime = 12
    Lighting.FogEnd = 9999
    Lighting.FogStart = 0
    for _, v in pairs(Workspace:GetDescendants()) do
        if v:IsA("ParticleEmitter") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") or
           v:IsA("BloomEffect") or v:IsA("BlurEffect") or v:IsA("DepthOfFieldEffect") or v:IsA("SunRaysEffect") then
            v.Enabled = false
        end
    end
    settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
    print("Anti-Lag → FPS should improve")
end)

toolsFolder:AddButton("Free AutoLift Gamepass", function()
    pcall(function()
        local gpFolder = ReplicatedStorage:FindFirstChild("gamepassIds")
        if gpFolder then
            local owned = LocalPlayer:FindFirstChild("ownedGamepasses") or Instance.new("Folder")
            owned.Name = "ownedGamepasses"
            owned.Parent = LocalPlayer
            for _, gp in pairs(gpFolder:GetChildren()) do
                local val = Instance.new("IntValue")
                val.Name = gp.Name
                val.Value = gp.Value
                val.Parent = owned
            end
            print("Gamepasses spoofed")
        else
            print("gamepassIds folder not found")
        end
    end)
end)

-- =============================================================
-- Teleport Tab (add remaining buttons as needed)
-- =============================================================
local function tpTo(pos)
    pcall(function()
        local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if hrp then hrp.CFrame = CFrame.new(pos) end
    end)
end

teleportTab:AddButton("To Spawn / Safe Area", function() tpTo(Vector3.new(-6, 5, -196)) end)
-- ... add your other teleport buttons here

-- =============================================================
-- Killer Tab
-- =============================================================
killFolder:AddSwitch("Kill Aura (All except whitelist)", function(state)
    getgenv().killAura = state
    if state then
        task.spawn(function()
            while getgenv().killAura do
                for _, plr in Players:GetPlayers() do
                    if plr ~= LocalPlayer and not whitelist[plr.Name] and plr.Character then
                        killPlayer(plr)
                    end
                end
                task.wait(0.08)
            end
        end)
    end
end)

local dropdown = killFolder:AddDropdown("Select Target", function(val)
    targetPlayerName = val:match("^([^|]+)") or val
end)

killFolder:AddSwitch("Auto Kill Selected", function(state)
    getgenv().killTarget = state
    if state and targetPlayerName then
        task.spawn(function()
            while getgenv().killTarget do
                local plr = Players:FindFirstChild(targetPlayerName)
                if plr then killPlayer(plr) end
                task.wait(0.08)
            end
            pcall(function()
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                    LocalPlayer.Character.Humanoid:UnequipTools()
                end
            end)
        end)
    end
end)

killFolder:AddInput("Add to Whitelist", {
    Placeholder = "Username or Display Name...",
    Callback = function(val)
        if val == "" then return end
        local lower = val:lower()
        for _, plr in Players:GetPlayers() do
            if plr.Name:lower() == lower or (plr.DisplayName and plr.DisplayName:lower() == lower) then
                whitelist[plr.Name] = true
                print("Whitelisted → " .. plr.Name)
                updateWhitelistDisplay()
                return
            end
        end
        print("Player not found: " .. val)
    end
})

killFolder:AddButton("Clear Whitelist", function()
    table.clear(whitelist)
    updateWhitelistDisplay()
    print("Whitelist cleared")
end)

-- Player list refresh
task.spawn(function()
    while true do
        local opts = {}
        for _, plr in Players:GetPlayers() do
            if plr ~= LocalPlayer then
                table.insert(opts, plr.Name .. " | " .. (plr.DisplayName or plr.Name))
            end
        end
        dropdown:SetValues(opts)
        task.wait(5)
    end
end)

-- =============================================================
-- Better Farming + Misc + Rebirth (your code, cleaned)
-- =============================================================
farmingTab:AddSwitch("Fast Grind / Strength", function(state)
    toggleFastStrength(state)
end)

miscTab:AddSwitch("Infinite Jump", function(state)
    getgenv().infJump = state
end)

UserInputService.JumpRequest:Connect(function()
    if getgenv().infJump and LocalPlayer.Character then
        local hum = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
    end
end)

-- Rebirth features (keep your existing ones - just make sure rEvents path is correct)
rebirthTab:AddSwitch("Auto Rebirth (Infinite)", function(state)
    getgenv().autoRebirth = state
    if state then
        task.spawn(function()
            while getgenv().autoRebirth do
                pcall(function()
                    local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
                    if rEvents and rEvents:FindFirstChild("rebirthRemote") then
                        rEvents.rebirthRemote:InvokeServer("rebirthRequest")
                    end
                end)
                task.wait(0.08)
            end
        end)
    end
end)

-- ... keep target rebirth, auto size, auto TP code as is (or paste it here)

print("Invincibles Priv Script - cleaned & Elerium v2 compatible")
