local playerTargetDropdown = killerTab:AddDropdown("PlayerTarget", {
    Title = "Select Player",
    Description = "Choose a player to target",
    Values = {},
    Multi = false,
    Default = nil,
    Callback = function(val)
        local username = val and val:match("^([^|]+)") or nil
        targetPlayer = username
        print("Target Player set to:", targetPlayer)
    end,
})

local function updatePlayerList()
    local vals = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            table.insert(vals, plr.Name .. " | " .. (plr.DisplayName or ""))
        end
    end
    playerTargetDropdown:SetValues(vals)
end

task.spawn(function()
    while true do
        updatePlayerList()
        task.wait(1)
    end
end)

-- =======================
-- Rebirth & Auto Rebirth (added features)
-- =======================

local function autoRebirth()
    task.spawn(function()
        while getgenv().NexusRunning do
            pcall(function()
                local rEvents = game:GetService("ReplicatedStorage"):WaitForChild("rEvents")
                local rebirthRemote = rEvents:FindFirstChild("rebirthRemote")
                if rebirthRemote then
                    rebirthRemote:InvokeServer("rebirthRequest")
                end
            end)
            task.wait(0.1)
        end
    end)
end

local function autoRebirthToTarget()
    task.spawn(function()
        while getgenv().NexusRunning and getgenv().AutoRebirth do
            pcall(function()
                local rebirths = LocalPlayer:FindFirstChild("leaderstats"):FindFirstChild("Rebirths")
                if rebirths and rebirths.Value >= targetRebirths then
                    getgenv().AutoRebirth = false
                else
                    local rEvents = game:GetService("ReplicatedStorage"):WaitForChild("rEvents")
                    local rebirthRemote = rEvents:FindFirstChild("rebirthRemote")
                    if rebirthRemote then
                        rebirthRemote:InvokeServer("rebirthRequest")
                    end
                end
            end)
            task.wait(0.12)
        end
    end)
end

-- =======================
-- Rebirth Toggles
-- =======================

-- Infinite auto rebirth
Tabs.Rebirth:AddSwitch("AutoRebirthInfinite", {
    Title = "Auto Rebirth (Infinite)",
    Default = false,
    Callback = function(s)
        getgenv().AutoRebirth = s
        if s then autoRebirth() end
    end,
})

-- Target rebirths input
Tabs.Rebirth:AddInput("TargetRebirths", {
    Title = "Target Rebirths",
    PlaceholderText = "Enter target rebirths",
    Numeric = true,
    Callback = function(value)
        local n = tonumber(value)
        if n and n > 0 then targetRebirths = n end
    end,
})

-- Auto rebirth to target
Tabs.Rebirth:AddSwitch("AutoRebirthToTarget", {
    Title = "Auto Rebirth to Target",
    Default = false,
    Callback = function(s)
        getgenv().AutoRebirthToTarget = s
        if s then autoRebirthToTarget() end
    end,
})

-- =======================
-- New: Fast Rebirth toggle
-- =======================
local fastRebirthActive = false
Tabs.Rebirth:AddToggle("FastRebirth", {
    Title = "Fast Rebirth",
    Default = false,
    Callback = function(s)
        fastRebirthActive = s
        if s then
            task.spawn(function()
                while fastRebirthActive do
                    pcall(function()
                        local rEvents = game:GetService("ReplicatedStorage"):WaitForChild("rEvents")
                        local rebirthRemote = rEvents:FindFirstChild("rebirthRemote")
                        if rebirthRemote then
                            rebirthRemote:InvokeServer("rebirthRequest")
                        end
                    end)
                    task.wait(0.1)
                end
            end)
        end
    end,
})

