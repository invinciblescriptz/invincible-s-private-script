	["Swift Samurai"] = "Equip All Swift Samurai",
	["Tribal Overlord"] = "Equip All Tribal Overlord",
	["Mighty Monster"] = "Equip All Mighty Monster",
	["Wild Wizard"] = "Equip All Wild Wizard"
}
