                while fastGrindActive do
                    pcall(function()
                        if muscleEvent then
                            muscleEvent:FireServer("punch", "rightHand")
                            muscleEvent:FireServer("punch", "leftHand")
                            -- Add other routines here if desired
                        end
                    end)
                    task.wait(0.09) -- attack interval
                end
            end)
        end
    end,
})

