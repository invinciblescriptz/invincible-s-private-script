-- Rebirth automation (assuming you define `startRebirthAutomation()` elsewhere)
local rebirthFolder = rebirthTab:AddFolder("Rebirth")
local function startRebirthAutomation()
    while _G.autoRebirth do
        pcall(function()
            game:GetService("ReplicatedStorage").rEvents.rebirthRemote:InvokeServer("rebirthRequest")
        end)
        wait(0.07)
    end
end

-- Auto Rebirth
rebirthFolder:AddSwitch("Auto Rebirth (Infinite)", function(state)
    _G.autoRebirth = state
    if state then
        task.spawn(startRebirthAutomation)
        print("Auto Rebirth enabled")
    else
        print("Auto Rebirth disabled")
    end
end)

-- Target Rebirth
local targetRebirthValue = 1
local targetRebirthLoop = nil
local function updateRebirthStats()
    local rebirths = LocalPlayer.leaderstats.Rebirths.Value
    print("Rebirths: " .. rebirths .. " / Target: " .. targetRebirthValue)
end

local rebirthTargetInput = rebirthTab:AddInput("TargetRebirthAmount", {
    Title = "Target Rebirth",
    Default = "1",
    Placeholder = "Enter target rebirth",
    Callback = function(val)
        targetRebirthValue = tonumber(val) or 1
        updateRebirthStats()
    end
})

local function autoRebirthTarget()
    while _G.autoRebirthTarget do
        pcall(function()
            if LocalPlayer.leaderstats.Rebirths.Value >= targetRebirthValue then
                _G.autoRebirthTarget = false
                break
            end
            game:GetService("ReplicatedStorage").rEvents.rebirthRemote:InvokeServer("rebirthRequest")
        end)
        wait(0.1)
    end
end

rebirthTab:AddSwitch("Auto Rebirth to Target", {
    Default = false,
    Callback = function(state)
        _G.autoRebirthTarget = state
        if state then
            task.spawn(autoRebirthTarget)
        end
    end
})

-- Toggle for "Fast Grind" (auto strength)
local fastStrengthRunning = false
local function startStrengthAutomation()
    -- implement your auto strength logic here
    while _G.isStrengthAuto do
        pcall(function()
            print("Auto Strength Running...")
        end)
        wait(1)
    end
end
local strengthToggle = farmingTab:AddToggle("UltimateStrength", {
    Default = false,
    Callback = function(state)
        _G.isStrengthAuto = state
        if state then
            task.spawn(startStrengthAutomation)
        end
    end
})

-- "Killer" tab for auto-killing
local killerTab = window:AddTab("Killer")
local killerFolder = killerTab:AddFolder("Auto Killer")

local function checkCharacter()
    if not LocalPlayer.Character then LocalPlayer.CharacterAdded:Wait() end
    return LocalPlayer.Character
end

local function killPlayer(target)
    local char = checkCharacter()
    if not char or not char:FindFirstChild("LeftHand") then return end
    local tChar = target.Character
    if tChar and tChar:FindFirstChild("HumanoidRootPart") and tChar:FindFirstChild("Humanoid") and tChar.Humanoid.Health > 0 then
        firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 0)
        firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 1)
        local punch = LocalPlayer.Backpack:FindFirstChild("Punch") or char:FindFirstChild("Punch")
        if punch and char:FindFirstChild("Humanoid") then char.Humanoid:EquipTool(punch) end
        if muscleEvent then
            muscleEvent:FireServer("punch", "leftHand")
            muscleEvent:FireServer("punch", "rightHand")
        end
    end
end

local killAura = false
killerFolder:AddSwitch("Kill All Players (Aura)", function(state)
    getgenv().killAura = state
    if state then
        task.spawn(function()
            while getgenv().killAura do
                pcall(function()
                    for _, plr in pairs(Players:GetPlayers()) do
                        if plr ~= LocalPlayer and plr.Character then
                            killPlayer(plr)
                        end
                    end
                end)
                task.wait(0.08)
            end
        end)
    end
end)

local targetPlayer = nil
local playerDropdown = killerFolder:AddDropdown("Select Target Player", {
    Title = "Single Target Kill",
    Values = {},
    Callback = function(selected)
        local name = selected:match("^([^|]+)") or selected
        targetPlayer = name
        print("Target set: " .. name)
    end
})

killerFolder:AddSwitch("Auto Kill Selected", {
    Default = false,
    Callback = function(state)
        getgenv().autoSingleKill = state
        if state and targetPlayer then
            task.spawn(function()
                while getgenv().autoSingleKill do
                    pcall(function()
                        local plr = Players:FindFirstChild(targetPlayer)
                        if plr then killPlayer(plr) end
                    end)
                    task.wait(0.08)
                end
            end)
        end
    end
})

-- Refresh player list periodically
task.spawn(function()
    while true do
        local list = {}
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer then
                table.insert(list, plr.Name .. " | " .. plr.DisplayName)
            end
        end
        playerDropdown:SetValues(list)
        task.wait(5)
    end
end)
