local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library"))()
local window = library:AddWindow("Invincibles Priv Script", {
    main_color = Color3.fromRGB(0, 255, 0),
    min_size = Vector2.new(470, 470),
    can_resize = false,
})

-- Services
local Players           = game:GetService("Players")
local LocalPlayer       = Players.LocalPlayer
local Workspace         = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService  = game:GetService("UserInputService")

local muscleEvent = LocalPlayer:WaitForChild("muscleEvent", 10)

-- =============================================
-- AUTO REP (unchanged, good)
-- =============================================
local function startAutoRep(toolName, toggleVar)
    task.spawn(function()
        while _G[toggleVar] do
            pcall(function()
                local char = LocalPlayer.Character
                if not char or not char:FindFirstChild("Humanoid") or char.Humanoid.Health <= 0 then
                    task.wait(1.5)
                    return
                end
                local tool = LocalPlayer.Backpack:FindFirstChild(toolName) or char:FindFirstChild(toolName)
                if tool and tool.Parent ~= char then
                    char.Humanoid:EquipTool(tool)
                end
                if muscleEvent then muscleEvent:FireServer("rep") end
            end)
            task.wait(0.025)
        end
        pcall(function()
            local char = LocalPlayer.Character
            if char then
                local eq = char:FindFirstChild(toolName)
                if eq then eq.Parent = LocalPlayer.Backpack end
            end
        end)
    end)
end

-- Tabs & Folders (unchanged)
local mainTab       = window:AddTab("Main")
local betterFarming = window:AddTab("Better Farming")
local teleportTab   = window:AddTab("Teleport")
local killTab       = window:AddTab("Kill")
local rebirthTab    = window:AddTab("Rebirth")
local miscTab       = window:AddTab("Misc")

local toolsFolder    = mainTab:AddFolder("Tools")
local rockFarmFolder = mainTab:AddFolder("Rock Farm")

-- Tools section (unchanged, looks fine)

-- ... (keep your existing Tools, Teleports, Kill Aura code here - no major issues)

-- =============================================
-- ROCK FARM (added debounce + safety)
-- =============================================
local selectedRock = "None"
local rockFarmRunning = false
local rockOptions = {
    "Tiny Island Rock", "Starter Island Rock", "Legend Beach Rock",
    "Frost Gym Rock", "Mythical Gym Rock", "Eternal Gym Rock",
    "Legend Gym Rock", "Muscle King Gym Rock",
}

rockFarmFolder:AddDropdown("Select Rock", {
    Values = rockOptions,
    Callback = function(value) selectedRock = value end
})

local durabilityRequirements = {
    ["Tiny Island Rock"]     = 0,
    ["Starter Island Rock"]  = 100,
    ["Legend Beach Rock"]    = 5000,
    ["Frost Gym Rock"]       = 150000,
    ["Mythical Gym Rock"]    = 400000,
    ["Eternal Gym Rock"]     = 750000,
    ["Legend Gym Rock"]      = 1000000,
    ["Muscle King Gym Rock"] = 5000000,
}

rockFarmFolder:AddSwitch("Auto Farm Selected Rock", function(state)
    rockFarmRunning = state
    if state and selectedRock ~= "None" then
        local reqDur = durabilityRequirements[selectedRock] or 0
        task.spawn(function()
            local lastTouch = 0
            while rockFarmRunning do
                task.wait(0.05)  -- Slightly slower to avoid rate limits
                pcall(function()
                    if not LocalPlayer:FindFirstChild("Durability") then return end
                    if LocalPlayer.Durability.Value < reqDur then return end

                    local char = LocalPlayer.Character
                    if not char or not char:FindFirstChild("HumanoidRootPart") then return end

                    for _, obj in pairs(Workspace.machinesFolder:GetDescendants()) do
                        if obj.Name == "neededDurability" and obj.Value == reqDur then
                            local rock = obj.Parent:FindFirstChild("Rock") or obj.Parent:FindFirstChildWhichIsA("BasePart")
                            if rock and (tick() - lastTouch > 0.4) then
                                lastTouch = tick()
                                firetouchinterest(rock, char.RightHand, 0) task.wait(0.02)
                                firetouchinterest(rock, char.RightHand, 1)
                                firetouchinterest(rock, char.LeftHand,  0) task.wait(0.02)
                                firetouchinterest(rock, char.LeftHand,  1)

                                -- Equip & punch
                                local humanoid = char:FindFirstChild("Humanoid")
                                if humanoid then
                                    for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
                                        if tool.Name == "Punch" then
                                            humanoid:EquipTool(tool)
                                            break
                                        end
                                    end
                                end
                                if muscleEvent then
                                    muscleEvent:FireServer("punch", "leftHand")
                                    muscleEvent:FireServer("punch", "rightHand")
                                end
                            end
                        end
                    end
                end)
            end
        end)
    end
end)

-- =============================================
-- REBIRTH (improved - fallback + proper cancel)
-- =============================================
local rebirthLoop = nil

local function tryRebirth()
    pcall(function()
        local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
        if rEvents and rEvents:FindFirstChild("rebirthRemote") then
            rEvents.rebirthRemote:InvokeServer("rebirthRequest")
            return true
        end
    end)
    pcall(function()
        if muscleEvent then
            muscleEvent:FireServer("rebirth")  -- common fallback in newer scripts
        end
    end)
end

rebirthTab:AddSwitch("Auto Rebirth (Infinite)", function(state)
    getgenv().autoRebirth = state
    if state then
        rebirthLoop = task.spawn(function()
            while getgenv().autoRebirth do
                tryRebirth()
                task.wait(0.1)
            end
        end)
    else
        if rebirthLoop then task.cancel(rebirthLoop) rebirthLoop = nil end
    end
end)

-- Target rebirth (fixed loop management)
local targetRebirth = 10  -- â† edit this to your goal
local targetRebirthLoop = nil

rebirthTab:AddSwitch("Auto Rebirth to Target (" .. targetRebirth .. ")", function(state)
    if state then
        targetRebirthLoop = task.spawn(function()
            while true do
                task.wait(0.12)
                if not LocalPlayer:FindFirstChild("leaderstats") or not LocalPlayer.leaderstats:FindFirstChild("Rebirths") then continue end
                if LocalPlayer.leaderstats.Rebirths.Value >= targetRebirth then
                    print("Target rebirth reached! Stopping.")
                    break
                end
                tryRebirth()
            end
            targetRebirthLoop = nil
        end)
    else
        if targetRebirthLoop then task.cancel(targetRebirthLoop) targetRebirthLoop = nil end
    end
end)

