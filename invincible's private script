-- Invincibles Priv Script (Cleaned & Fixed - Feb 2026 version)
-- Assumes: elerium-v2-ui-library

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library"))()
local window = library:AddWindow("Invincibles Priv Script", {
    main_color = Color3.fromRGB(50, 50, 50),
    min_size = Vector2.new(470, 470),
    can_resize = false,
})

-- Services
local Players            = game:GetService("Players")
local LocalPlayer        = Players.LocalPlayer
local Workspace          = game:GetService("Workspace")
local ReplicatedStorage  = game:GetService("ReplicatedStorage")
local UserInputService   = game:GetService("UserInputService")
local RunService         = game:GetService("RunService")
local VirtualInputManager = game:GetService("VirtualInputManager")

local muscleEvent = LocalPlayer:WaitForChild("muscleEvent", 8)
local rEvents     = ReplicatedStorage:WaitForChild("rEvents", 5)

-- State
local states = {
    AutoPushups     = false,
    AutoWeight      = false,
    AutoHandstands  = false,
    FastPunch       = false,
    InfiniteJump    = false,
    KillAura        = false,
    AutoKillSelected= false,
    FastStrength    = false,
    HidePets        = false,
}

local targetRebirths     = 1
local selectedTargetPlayer = nil
getgenv().rockFarmActive = false
getgenv().selectedRockDurability = 0
getgenv().killAura       = false
getgenv().autoKillSelected = false

-- Tabs
local mainTab     = window:AddTab("Main")
local fastFarmTab = window:AddTab("Fast Farming")
local rebirthTab  = window:AddTab("Rebirth")
local teleportTab = window:AddTab("Teleport")
local killTab     = window:AddTab("Kill")
local miscTab     = window:AddTab("Misc")

local toolsFolder   = mainTab:AddFolder("Tools")
local rockFarmFolder= mainTab:AddFolder("Rock Farm")
local killFolder    = killTab:AddFolder("Auto Kill")

-- =============================================================
-- Helpers
-- =============================================================
local function tpTo(pos)
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if hrp then
        hrp.CFrame = CFrame.new(pos)
    end
end

local function equipTool(toolName)
    local char = LocalPlayer.Character
    if not char then return end
    local tool = LocalPlayer.Backpack:FindFirstChild(toolName) or char:FindFirstChild(toolName)
    if tool and tool.Parent ~= char then
        char.Humanoid:EquipTool(tool)
    end
end

local function unequipAllPets()
    local petsFolder = LocalPlayer:FindFirstChild("petsFolder")
    if not petsFolder then return end
    for _, folder in petsFolder:GetChildren() do
        if folder:IsA("Folder") then
            for _, pet in folder:GetChildren() do
                rEvents.equipPetEvent:FireServer("unequipPet", pet)
            end
        end
    end
end

local function equipPet(petName)
    unequipAllPets()
    task.wait(0.15)
    local unique = LocalPlayer.petsFolder:FindFirstChild("Unique")
    if not unique then return end
    for _, pet in unique:GetChildren() do
        if pet.Name == petName then
            rEvents.equipPetEvent:FireServer("equipPet", pet)
            return
        end
    end
end

-- =============================================================
-- Auto Rep / Tools
-- =============================================================
local function startAutoRep(toolName, stateKey)
    task.spawn(function()
        while states[stateKey] do
            pcall(function()
                local char = LocalPlayer.Character
                if not char or not char:FindFirstChild("Humanoid") or char.Humanoid.Health <= 0 then
                    task.wait(1.5) return
                end
                equipTool(toolName)
                if muscleEvent then muscleEvent:FireServer("rep") end
            end)
            task.wait(0.03)
        end
    end)
end

toolsFolder:AddSwitch("Auto Pushups", function(s)
    states.AutoPushups = s
    if s then startAutoRep("Pushups", "AutoPushups") end
end)

toolsFolder:AddSwitch("Auto Weight", function(s)
    states.AutoWeight = s
    if s then startAutoRep("Weight", "AutoWeight") end
end)

toolsFolder:AddSwitch("Auto Handstands", function(s)
    states.AutoHandstands = s
    if s then startAutoRep("Handstands", "AutoHandstands") end
end)

-- Fast Punch
toolsFolder:AddSwitch("Auto Punch (Fast)", function(s)
    states.FastPunch = s
    if s then
        task.spawn(function()
            while states.FastPunch do
                pcall(function()
                    if muscleEvent then
                        muscleEvent:FireServer("punch", "rightHand")
                        muscleEvent:FireServer("punch", "leftHand")
                    end
                    equipTool("Punch")
                    local char = LocalPlayer.Character
                    local punch = char and char:FindFirstChild("Punch")
                    if punch then
                        local atk = punch:FindFirstChild("attackTime")
                        if atk and atk:IsA("NumberValue") then atk.Value = 0.01 end
                        punch:Activate()
                    end
                end)
                task.wait(0.035)
            end
        end)
    end
end)

-- Fast Strength (merged from multiple sections)
local fastStrConn
fastFarmTab:AddSwitch("Fast Grind / Strength", function(s)
    states.FastStrength = s
    if s then
        if fastStrConn then fastStrConn:Disconnect() end
        fastStrConn = RunService.Heartbeat:Connect(function()
            pcall(function()
                if muscleEvent then
                    muscleEvent:FireServer("rep")
                    muscleEvent:FireServer("punch", "rightHand")
                    muscleEvent:FireServer("punch", "leftHand")
                end
                local char = LocalPlayer.Character
                local tool = char and char:FindFirstChildWhichIsA("Tool")
                if tool then tool:Activate() end
            end)
        end)
    else
        if fastStrConn then fastStrConn:Disconnect() fastStrConn = nil end
    end
end)

-- =============================================================
-- Rock Farm
-- =============================================================
task.spawn(function()
    while true do
        if not getgenv().rockFarmActive or getgenv().selectedRockDurability == 0 then
            task.wait(0.6) continue
        end
        pcall(function()
            local char = LocalPlayer.Character
            if not char or char.Humanoid.Health <= 0 then task.wait(1) return end

            local dur = LocalPlayer:FindFirstChild("Durability")
            if not dur or dur.Value < getgenv().selectedRockDurability then
                task.wait(0.5) return
            end

            equipTool("Punch")

            for _, obj in Workspace.machinesFolder:GetDescendants() do
                if obj.Name == "neededDurability" and obj:IsA("IntValue") and obj.Value == getgenv().selectedRockDurability then
                    local rock = obj.Parent:FindFirstChild("Rock")
                    if rock and rock:IsA("BasePart") then
                        if char:FindFirstChild("RightHand") then
                            firetouchinterest(rock, char.RightHand, 0) task.wait(0.02) firetouchinterest(rock, char.RightHand, 1)
                        end
                        if char:FindFirstChild("LeftHand") then
                            firetouchinterest(rock, char.LeftHand, 0) task.wait(0.02) firetouchinterest(rock, char.LeftHand, 1)
                        end
                        if muscleEvent then
                            muscleEvent:FireServer("punch", "leftHand")
                            muscleEvent:FireServer("punch", "rightHand")
                        end
                    end
                end
            end
        end)
        task.wait(0.14)
    end
end)

-- Rock switches (only one active at a time)
local rockSwitches = {
    {name = "Tiny Island Rock (0)",           dur = 0},
    {name = "Starter Island Rock (100)",       dur = 100},
    {name = "Legend Beach Rock (5000)",        dur = 5000},
    {name = "Frost Gym Rock (150k)",           dur = 150000},
    {name = "Mythical Gym Rock (400k)",        dur = 400000},
    {name = "Eternal Gym Rock (750k)",         dur = 750000},
    {name = "Legend Gym Rock (1M)",            dur = 1000000},
    {name = "Muscle King Gym Rock (5M)",       dur = 5000000},
    {name = "Ancient Jungle Rock (10M)",       dur = 10000000},
}

for _, sw in rockSwitches do
    rockFarmFolder:AddSwitch("Farm " .. sw.name, function(s)
        getgenv().rockFarmActive = s
        getgenv().selectedRockDurability = s and sw.dur or 0
    end)
end

rockFarmFolder:AddButton("STOP All Rock Farming", function()
    getgenv().rockFarmActive = false
    getgenv().selectedRockDurability = 0
end)

-- =============================================================
-- Rebirth section (cleaned)
-- =============================================================
local rebirthRunning = false
local rebirthTargetRunning = false

rebirthTab:AddInput("Target Rebirths", {
    PlaceholderText = "Enter number",
    Numeric = true,
    Callback = function(val)
        local n = tonumber(val)
        if n and n >= 1 then
            targetRebirths = n
        end
    end
})

rebirthTab:AddSwitch("Auto Rebirth (Target)", function(s)
    rebirthTargetRunning = s
    if s then
        task.spawn(function()
            local leaderstats = LocalPlayer:WaitForChild("leaderstats")
            local rebirthsVal = leaderstats:WaitForChild("Rebirths")
            local rebirthRemote = rEvents:WaitForChild("rebirthRemote")

            while rebirthTargetRunning do
                if rebirthsVal.Value >= targetRebirths then
                    rebirthTargetRunning = false
                    print("Target rebirths reached!")
                    break
                end
                pcall(function()
                    rebirthRemote:InvokeServer("rebirthRequest")
                end)
                task.wait(0.8)
            end
        end)
    end
end)

rebirthTab:AddSwitch("Auto Rebirth (Infinite)", function(s)
    rebirthRunning = s
    if s then
        task.spawn(function()
            local rebirthRemote = rEvents:WaitForChild("rebirthRemote")
            while rebirthRunning do
                pcall(function()
                    rebirthRemote:InvokeServer("rebirthRequest")
                end)
                task.wait(0.6)
            end
        end)
    end
end)

rebirthTab:AddSwitch("Auto Size 2", function(s)
    if s then
        task.spawn(function()
            local remote = rEvents:WaitForChild("changeSpeedSizeRemote")
            while s do
                pcall(function()
                    remote:InvokeServer("changeSize", 2)
                end)
                task.wait(0.4)
            end
        end)
    end
end)

-- =============================================================
-- Teleports
-- =============================================================
teleportTab:AddButton("To Spawn",           function() tpTo(Vector3.new(-6,    5, -196))   end)
teleportTab:AddButton("Secret Area",        function() tpTo(Vector3.new(1947,  2, 6191))  end)
teleportTab:AddButton("Tiny Area",          function() tpTo(Vector3.new(-39,   6, 1886))  end)
teleportTab:AddButton("Frozen Area",        function() tpTo(Vector3.new(-2623, 6, -409))  end)
teleportTab:AddButton("Mythical Area",      function() tpTo(Vector3.new(2251,  6, 1073))  end)
teleportTab:AddButton("Inferno Area",       function() tpTo(Vector3.new(-6759, 6, -1285)) end)
teleportTab:AddButton("Legend Area",        function() tpTo(Vector3.new(4603,990,-3898)) end)
teleportTab:AddButton("Muscle King Area",   function() tpTo(Vector3.new(-8626,16,-5730)) end)
teleportTab:AddButton("Jungle Area",        function() tpTo(Vector3.new(-8642, 6, 2381)) end)

-- =============================================================
-- Kill / Aura
-- =============================================================
local function killPlayer(targetPlr)
    pcall(function()
        local char = LocalPlayer.Character
        local tChar = targetPlr.Character
        if not (char and tChar and tChar:FindFirstChild("HumanoidRootPart") and tChar.Humanoid.Health > 0) then return end

        local hand = char:FindFirstChild("LeftHand") or char:FindFirstChild("RightHand")
        if hand then
            firetouchinterest(tChar.HumanoidRootPart, hand, 0)
            task.wait(0.03)
            firetouchinterest(tChar.HumanoidRootPart, hand, 1)
        end

        equipTool("Punch")
        if muscleEvent then
            muscleEvent:FireServer("punch", "leftHand")
            muscleEvent:FireServer("punch", "rightHand")
        end
    end)
end

killFolder:AddSwitch("Kill Aura (All)", function(s)
    getgenv().killAura = s
    if s then
        task.spawn(function()
            while getgenv().killAura do
                for _, plr in Players:GetPlayers() do
                    if plr ~= LocalPlayer and plr.Character then
                        killPlayer(plr)
                    end
                end
                task.wait(0.09)
            end
        end)
    end
end)

-- Target dropdown
local playerDropdown = killFolder:AddDropdown("Select Target", function(val)
    selectedTargetPlayer = val:match("^([^|]+)") or val
end)

task.spawn(function()
    while true do
        local opts = {}
        for _, plr in Players:GetPlayers() do
            if plr ~= LocalPlayer then
                table.insert(opts, plr.Name .. " | " .. (plr.DisplayName or plr.Name))
            end
        end
        playerDropdown:SetValues(opts)
        task.wait(4)
    end
end)

killFolder:AddSwitch("Auto Kill Selected", function(s)
    getgenv().autoKillSelected = s
    if s and selectedTargetPlayer then
        task.spawn(function()
            while getgenv().autoKillSelected do
                local plr = Players:FindFirstChild(selectedTargetPlayer)
                if plr then killPlayer(plr) end
                task.wait(0.09)
            end
        end)
    end
end)

-- =============================================================
-- Misc
-- =============================================================
miscTab:AddSwitch("Infinite Jump", function(s)
    states.InfiniteJump = s
end)

UserInputService.JumpRequest:Connect(function()
    if states.InfiniteJump and LocalPlayer.Character then
        local hum = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
    end
end)

miscTab:AddSwitch("Hide Pets", function(s)
    states.HidePets = s
    local ev = rEvents:FindFirstChild("showPetsEvent")
    if ev then
        ev:FireServer(s and "hidePets" or "showPets")
    end
end)

toolsFolder:AddButton("Anti Lag (FPS Boost)", function()
    local Lighting = game:GetService("Lighting")
    Lighting.GlobalShadows = false
    Lighting.Brightness = 1
    Lighting.ClockTime = 12
    Lighting.FogEnd = 9999
    for _, v in Workspace:GetDescendants() do
        if v:IsA("ParticleEmitter") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") or
           v:IsA("BloomEffect") or v:IsA("BlurEffect") or v:IsA("DepthOfFieldEffect") or v:IsA("SunRaysEffect") then
            v.Enabled = false
        end
    end
    settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
end)

toolsFolder:AddButton("Spoof AutoLift Gamepass", function()
    pcall(function()
        local gpFolder = ReplicatedStorage:FindFirstChild("gamepassIds")
        if gpFolder then
            local owned = LocalPlayer:FindFirstChild("ownedGamepasses") or Instance.new("Folder")
            owned.Name = "ownedGamepasses"
            owned.Parent = LocalPlayer
            for _, gp in gpFolder:GetChildren() do
                local val = Instance.new("IntValue")
                val.Name = gp.Name
                val.Value = gp.Value
                val.Parent = owned
            end
        end
    end)
end)

print("Invincibles Priv Script - cleaned & fixed (Feb 2026)")
