-- Kill All Players
local function checkCharacter()
    if not LocalPlayer.Character then LocalPlayer.CharacterAdded:Wait() end
    return LocalPlayer.Character
end

local function killPlayer(target)
    local char = checkCharacter()
    if not char or not char:FindFirstChild("LeftHand") then return end
    local tChar = target.Character
    if tChar and tChar:FindFirstChild("HumanoidRootPart") and tChar:FindFirstChild("Humanoid") and tChar.Humanoid.Health > 0 then
        firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 0)
        firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 1)
        local punch = LocalPlayer.Backpack:FindFirstChild("Punch") or char:FindFirstChild("Punch")
        if punch and char:FindFirstChild("Humanoid") then
            char.Humanoid:EquipTool(punch)
        end
        if muscleEvent then
            muscleEvent:FireServer("punch", "leftHand")
            muscleEvent:FireServer("punch", "rightHand")
        end
    end
end

local killAura = false
local function killAll()
    while killAura do
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer and plr.Character then
                killPlayer(plr)
            end
        end
        wait(0.1)
    end
end

local killFolder = KillTab:AddFolder("Auto Kill")
killFolder:AddSwitch("Kill All Players", function(state)
    killAura = state
    if state then
        task.spawn(killAll)
    end
end)

local targetPlayer = nil
local dropdown = killFolder:AddDropdown("Select Target Player", {
    Values = {},
    Callback = function(selected)
        targetPlayer = selected
        print("Target Player: " .. selected)
    end
})
killFolder:AddSwitch("Auto Kill Selected", function(state)
    getgenv().autoKillSelected = state
    if state and targetPlayer then
        task.spawn(function()
            while getgenv().autoKillSelected do
                local target = Players:FindFirstChild(targetPlayer)
                if target then killPlayer(target) end
                wait(0.08)
            end
        end)
    end
end)

-- Update Player List
task.spawn(function()
    while true do
        local list = {}
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer then
                table.insert(list, plr.Name .. " | " .. plr.DisplayName)
            end
        end
        pcall(function()
            dropdown:SetValues(list)
        end)
        wait(5)
    end
end)

-- Rock Farm
local function equipPunch()
    pcall(function()
        local char = LocalPlayer.Character
        if char then
            local punch = LocalPlayer.Backpack:FindFirstChild("Punch")
            if punch then
                char.Humanoid:EquipTool(punch)
            end
        end
        if muscleEvent then
            muscleEvent:FireServer("punch", "leftHand")
            muscleEvent:FireServer("punch", "rightHand")
        end
    end)
end

local function farmRocks(durability)
    while _G.autoRockFarm do
        for _, v in pairs(workspace:FindFirstChild("machinesFolder") and workspace.machinesFolder:GetDescendants() or {}) do
            if v.Name == "neededDurability" and v.Value == durability then
                local rock = v.Parent:FindFirstChild("Rock")
                local char = LocalPlayer.Character
                if rock and char and char:FindFirstChild("RightHand") and char:FindFirstChild("LeftHand") then
                    firetouchinterest(rock, char.RightHand, 0)
                    firetouchinterest(rock, char.RightHand, 1)
                    firetouchinterest(rock, char.LeftHand, 0)
                    firetouchinterest(rock, char.LeftHand, 1)
                    equipPunch()
                end
            end
        end
        -- Auto punch while farming
        if _G.FastPunch then
            if muscleEvent then
                muscleEvent:FireServer("punch", "rightHand")
                muscleEvent:FireServer("punch", "leftHand")
            end
            local ch = LocalPlayer.Character
            if ch then
                local punch = ch:FindFirstChild("Punch")
                if punch then punch:Activate() end
            end
        end
        wait(0.15)
    end
end

local rockFarmTab = window:AddTab("Rock Farm")
rockFarmTab:AddSwitch("Farm Tiny Island Rock", {
    Default = false,
    Callback = function(val)
        _G.autoRockFarm = val
        if val then farmRocks(0) end
    end
})
-- You can add more toggles for other rocks similarly.

-- Auto Strength (better farming)
local fastStrength = false
local function startStrengthAutomation()
    while fastStrength do
        pcall(function()
            local petsFolder = LocalPlayer:WaitForChild("petsFolder", 5)
            if petsFolder then
                local uniquePets = petsFolder:FindFirstChild("Unique")
                if uniquePets then
                    for _, pet in pairs(uniquePets:GetChildren()) do
                        if pet.Name == "Swift Samurai" then -- change to your best pet
                            local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
                            if rEvents and rEvents:FindFirstChild("equipPetEvent") then
                                rEvents.equipPetEvent:FireServer("equipPet", pet)
                            end
                        end
                    end
                end
            end
            -- Equip punch
            equipPunch()
            -- Repeated reps
            for i=1,30 do
                if muscleEvent then muscleEvent:FireServer("rep") end
            end
        end)
        wait(0.05)
    end
end

betterFarming:AddSwitch("Fast Strength Gain", function(state)
    fastStrength = state
    if state then startStrengthAutomation() end
end)

-- Infinite Jump
Misc:AddSwitch("Infinite Jump", function(st)
    getgenv().infJump = st
end)
UserInputService.JumpRequest:Connect(function()
    if getgenv().infJump and LocalPlayer.Character then
        LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
    end
end)

-- Rebirth
rebirthTab:AddSwitch("Auto Rebirth (Infinite)", function(st)
    getgenv().autoRebirth = st
    if st then
        task.spawn(function()
            while getgenv().autoRebirth do
                pcall(function()
                    local rEvents = ReplicatedStorage:WaitForChild("rEvents", 5)
                    if rEvents and rEvents:FindFirstChild("rebirthRemote") then
                        rEvents.rebirthRemote:InvokeServer("rebirthRequest")
                    end
                end)
                wait(0.1)
            end
        end)
    end
end)

rebirthTab:AddSwitch("Auto Size 2", function(st)
    if st then
        getgenv().autoSizeLoop = task.spawn(function()
            while true do
                pcall(function()
                    local rEvents = ReplicatedStorage:WaitForChild("rEvents", 5)
                    if rEvents and rEvents:FindFirstChild("changeSpeedSizeRemote") then
                        rEvents.changeSpeedSizeRemote:InvokeServer("changeSize", 2)
                    end
                end)
                wait(0.05)
            end
        end)
    else
        if getgenv().autoSizeLoop then task.cancel(getgenv().autoSizeLoop) end
    end
end)

rebirthTab:AddSwitch("Auto Teleport to Muscle King", function(st)
    if st then
        getgenv().kingTeleportLoop = task.spawn(function()
            while true do
                pcall(function()
                    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if hrp then hrp.CFrame = CFrame.new(-8646, 17, -5738) end
                end)
                wait(0.05)
            end
        end)
    else
        if getgenv().kingTeleportLoop then task.cancel(getgenv().kingTeleportLoop) end
    end
end)

rebirthTab:AddSwitch("Hide All Frames", function(st)
    pcall(function()
        for _, obj in pairs(ReplicatedStorage:GetDescendants()) do
            if obj:IsA("Frame") or obj.Name:match("Frame$") then obj.Visible = not st end
        end
        for _, obj in pairs(LocalPlayer.PlayerGui:GetDescendants()) do
            if obj:IsA("Frame") then obj.Visible = not st end
        end
    end)
end)

-- Fast Rebirth Spam
local fastRebirth = false
local function startRebirthSpam()
    while fastRebirth do
        pcall(function()
            local rEvents = ReplicatedStorage:WaitForChild("rEvents", 5)
            if rEvents and rEvents:FindFirstChild("rebirthRemote") then
                rEvents.rebirthRemote:InvokeServer("rebirthRequest")
            end
        end)
        wait(0.07)
    end
end

rebirthTab:AddSwitch("Fast Rebirth Spam", function(st)
    fastRebirth = st
    if st then
        task.spawn(startRebirthSpam)
    end
end)

-- Auto Rebirth to Target (set target in variable)
local targetRebirthValue = 1 -- change this to your desired target
local targetRebirthRunning = false
rebirthTab:AddSwitch("Auto Rebirth to Target", function(st)
    targetRebirthRunning = st
    if st then
        task.spawn(function()
            print("Target Rebirth started for amount: " .. targetRebirthValue)
            while targetRebirthRunning do
                pcall(function()
                    local current = LocalPlayer.leaderstats.Rebirths.Value
                    if current >= targetRebirthValue then
                        targetRebirthRunning = false
                        print("Target Reached! Stopping.")
                        return
                    end
                    local rEvents = ReplicatedStorage:WaitForChild("rEvents", 5)
                    if rEvents and rEvents:FindFirstChild("rebirthRemote") then
                        rEvents.rebirthRemote:InvokeServer("rebirthRequest")
                    end
                end)
                wait(0.1)
            end
        end)
    end
end)

print("Script loaded! Use the UI to activate features.")
