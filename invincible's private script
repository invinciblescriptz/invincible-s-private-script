-- =============================================================
-- Better Farming + Misc + Rebirth
-- =============================================================
betterFarmingTab:AddSwitch("Fast Grind / Strength", function(state)
    toggleFastStrength(state)
end)

miscTab:AddSwitch("Infinite Jump", function(state)
    infJump = state
end)

UserInputService.JumpRequest:Connect(function()
    if infJump and LocalPlayer.Character then
        local hum = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
    end
end)

rebirthTab:AddSwitch("Auto Rebirth (Infinite)", function(state)
    autoRebirth = state
    if state then
        task.spawn(function()
            while autoRebirth do
                pcall(function()
                    local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
                    if rEvents and rEvents:FindFirstChild("rebirthRemote") then
                        rEvents.rebirthRemote:InvokeServer("rebirthRequest")
                    end
                end)
                task.wait(0.08)
            end
        end)
    end
end)

rebirthTab:AddInput("Target Rebirths", function(value)
    local num = tonumber(value)
    targetRebirths = num and num > 0 and num or 1
    print("Target rebirths set to: " .. targetRebirths)
end)

rebirthTab:AddSwitch("Auto Rebirth to Target", function(state)
    autoRebirthTarget = state
    if state then
        task.spawn(function()
            while autoRebirthTarget do
                pcall(function()
                    if LocalPlayer.leaderstats and LocalPlayer.leaderstats:FindFirstChild("Rebirths") then
                        if LocalPlayer.leaderstats.Rebirths.Value >= targetRebirths then
                            autoRebirthTarget = false
                            print("Target rebirth reached → stopped")
                            return
                        end
                    end
                    local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
                    if rEvents and rEvents:FindFirstChild("rebirthRemote") then
                        rEvents.rebirthRemote:InvokeServer("rebirthRequest")
                    end
                end)
                task.wait(0.1)
            end
        end)
    end
end)

rebirthTab:AddSwitch("Auto Size 2", function(state)
    if state then
        getgenv().sizeLoop = task.spawn(function()
            while true do
                pcall(function()
                    local rEvents = ReplicatedStorage:FindFirstChild("rEvents")
                    if rEvents and rEvents:FindFirstChild("changeSpeedSizeRemote") then
                        rEvents.changeSpeedSizeRemote:InvokeServer("changeSize", 2)
                    end
                end)
                task.wait(0.05)
            end
        end)
    else
        if getgenv().sizeLoop then task.cancel(getgenv().sizeLoop) end
    end
end)

rebirthTab:AddSwitch("Auto TP Muscle King", function(state)
    if state then
        getgenv().kingTpLoop = task.spawn(function()
            while true do
                pcall(function()
                    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if hrp then hrp.CFrame = CFrame.new(-8646, 17, -5738) end
                end)
                task.wait(0.1)
            end
        end)
    else
        if getgenv().kingTpLoop then task.cancel(getgenv().kingTpLoop) end
    end
end)

