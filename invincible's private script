-- // Invincibles Priv Script - Fixed & Cleaned (Elerium v2 UI)
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library"))()
local window = library:AddWindow("Invincibles Priv Script", {
    main_color = Color3.fromRGB(0, 255, 0),
    min_size = Vector2.new(470, 470),
    can_resize = false,
})

-- Services
local Players           = game:GetService("Players")
local LocalPlayer       = Players.LocalPlayer
local Workspace         = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService  = game:GetService("UserInputService")

local muscleEvent = LocalPlayer:WaitForChild("muscleEvent", 8)

-- =============================================
--  Auto Rep / Grind Functions
-- =============================================
local function startAutoRep(toolName, toggleVar)
    task.spawn(function()
        while _G[toggleVar] do
            pcall(function()
                local char = LocalPlayer.Character
                if not char or not char:FindFirstChild("Humanoid") or char.Humanoid.Health <= 0 then
                    task.wait(1.5)
                    return
                end

                local tool = LocalPlayer.Backpack:FindFirstChild(toolName) or char:FindFirstChild(toolName)
                if tool and tool.Parent ~= char then
                    char.Humanoid:EquipTool(tool)
                end

                if muscleEvent then
                    muscleEvent:FireServer("rep")
                end
            end)
            task.wait(0.025)
        end

        -- Unequip when disabled
        pcall(function()
            local char = LocalPlayer.Character
            if char then
                local eq = char:FindFirstChild(toolName)
                if eq then eq.Parent = LocalPlayer.Backpack end
            end
        end)
    end)
end

-- =============================================
--  Tabs & Folders
-- =============================================
local main          = window:AddTab("Main")
local betterFarming = window:AddTab("Better Farming")
local rebirthTab    = window:AddTab("Rebirth")
local teleportTab   = window:AddTab("Teleport")
local killTab       = window:AddTab("Kill")
local miscTab       = window:AddTab("Misc")
local inventoryTab  = window:AddTab("Inventory")

local ToolsFolder     = main:AddFolder("Tools")
local RockFarmFolder  = main:AddFolder("Rock Farm")
local MiscFolder      = miscTab:AddFolder("Misc")

-- =============================================
--  Main → Tools
-- =============================================
ToolsFolder:AddSwitch("Auto Pushups",    function(s) _G.AutoPushups    = s if s then startAutoRep("Pushups",    "AutoPushups")    end end)
ToolsFolder:AddSwitch("Auto Weight",     function(s) _G.AutoWeight     = s if s then startAutoRep("Weight",     "AutoWeight")     end end)
ToolsFolder:AddSwitch("Auto Handstands", function(s) _G.AutoHandstands = s if s then startAutoRep("Handstands", "AutoHandstands") end end)

ToolsFolder:AddSwitch("Fast Punch", function(state)
    _G.FastPunch = state
    if state then
        task.spawn(function()
            while _G.FastPunch do
                pcall(function()
                    if muscleEvent then
                        muscleEvent:FireServer("punch", "rightHand")
                        muscleEvent:FireServer("punch", "leftHand")
                    end
                    local char = LocalPlayer.Character
                    if char and char:FindFirstChild("Punch") then
                        char.Punch:Activate()
                    end
                end)
                task.wait(0.07)
            end
        end)
    end
end)

ToolsFolder:AddButton("Anti Lag (Boost FPS)", function()
    local Lighting = game:GetService("Lighting")
    Lighting.GlobalShadows = false
    Lighting.Brightness = 1
    Lighting.ClockTime = 12
    Lighting.FogEnd = 9999
    Lighting.FogStart = 0

    for _, v in pairs(Workspace:GetDescendants()) do
        if v:IsA("ParticleEmitter") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") or
           v:IsA("BloomEffect") or v:IsA("BlurEffect") or v:IsA("DepthOfFieldEffect") or v:IsA("SunRaysEffect") then
            v.Enabled = false
        end
    end

    settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
    print("Anti-Lag → FPS should improve (Shift+F5 to check)")
end)

ToolsFolder:AddButton("Free AutoLift Gamepass", function()
    pcall(function()
        local gpFolder = ReplicatedStorage:FindFirstChild("gamepassIds")
        if gpFolder then
            local owned = LocalPlayer:FindFirstChild("ownedGamepasses") or Instance.new("Folder", LocalPlayer)
            owned.Name = "ownedGamepasses"
            for _, gp in pairs(gpFolder:GetChildren()) do
                local val = Instance.new("IntValue")
                val.Name = gp.Name
                val.Value = gp.Value
                val.Parent = owned
            end
            print("Gamepasses spoofed (may need relog)")
        else
            warn("gamepassIds folder not found")
        end
    end)
end)

-- =============================================
--  Teleport Tab
-- =============================================
local function tpTo(pos)
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if hrp then hrp.CFrame = CFrame.new(pos) end
end

teleportTab:AddButton("Spawn / Safe",         function() tpTo(Vector3.new(-6,    5,   -196))   end)
teleportTab:AddButton("Secret Area",          function() tpTo(Vector3.new(1947,  2,   6191))   end)
teleportTab:AddButton("Tiny Area",            function() tpTo(Vector3.new(-39,   6,   1886))   end)
teleportTab:AddButton("Frozen Area",          function() tpTo(Vector3.new(-2623, 6,   -409))   end)
teleportTab:AddButton("Mythical Area",        function() tpTo(Vector3.new(2251,  6,   1073))   end)
teleportTab:AddButton("Inferno Area",         function() tpTo(Vector3.new(-6759, 6,   -1285))  end)
teleportTab:AddButton("Legend Area",          function() tpTo(Vector3.new(4603,  990, -3898))  end)
teleportTab:AddButton("Muscle King Area",     function() tpTo(Vector3.new(-8626, 16,  -5730))  end)
teleportTab:AddButton("Jungle Area",          function() tpTo(Vector3.new(-8642, 6,   2381))   end)
teleportTab:AddButton("Lava Brawl",           function() tpTo(Vector3.new(4471,  119, -8836))  end)
teleportTab:AddButton("Desert Brawl",         function() tpTo(Vector3.new(960,   17,  -7398))  end)
teleportTab:AddButton("Beach Brawl",          function() tpTo(Vector3.new(-1849, 20,  -6335))  end)

-- =============================================
--  Kill Tab
-- =============================================
local KillFolder = killTab:AddFolder("Auto Kill")

local function killPlayer(target)
    local char = LocalPlayer.Character
    if not char or not char:FindFirstChild("LeftHand") then return end

    local tChar = target.Character
    if tChar and tChar:FindFirstChild("HumanoidRootPart") and tChar.Humanoid.Health > 0 then
        firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 0)
        firetouchinterest(tChar.HumanoidRootPart, char.LeftHand, 1)

        local punch = LocalPlayer.Backpack:FindFirstChild("Punch") or char:FindFirstChild("Punch")
        if punch and char:FindFirstChild("Humanoid") then
            char.Humanoid:EquipTool(punch)
        end

        if muscleEvent then
            muscleEvent:FireServer("punch", "leftHand")
            muscleEvent:FireServer("punch", "rightHand")
        end
    end
end

KillFolder:AddSwitch("Kill Aura (All)", function(state)
    getgenv().killAura = state
    if state then
        task.spawn(function()
            while getgenv().killAura do
                for _, plr in Players:GetPlayers() do
                    if plr ~= LocalPlayer and plr.Character then
                        pcall(killPlayer, plr)
                    end
                end
                task.wait(0.08)
            end
        end)
    end
end)

local targetPlayerName = nil
local playerDropdown = KillFolder:AddDropdown("Target Player", {
    Values = {},
    Callback = function(selected)
        targetPlayerName = selected:match("^([^|]+)") or selected
    end
})

KillFolder:AddSwitch("Auto Kill Selected", function(state)
    getgenv().autoKillTarget = state
    if state and targetPlayerName then
        task.spawn(function()
            while getgenv().autoKillTarget do
                local target = Players:FindFirstChild(targetPlayerName)
                if target then pcall(killPlayer, target) end
                task.wait(0.08)
            end
        end)
    end
end)

-- Auto-refresh player list
task.spawn(function()
    while true do
        local list = {}
        for _, plr in Players:GetPlayers() do
            if plr ~= LocalPlayer then
                table.insert(list, plr.Name .. " | " .. plr.DisplayName)
            end
        end
        playerDropdown:SetValues(list)
        task.wait(4)
    end
end)

-- =============================================
--  Rebirth Tab (cleaned)
-- =============================================
local targetRebirth = 1

rebirthTab:AddInput("Target Rebirths", {
    Placeholder = "e.g. 50",
    Numeric = true,
    Callback = function(val)
        targetRebirth = tonumber(val) or 1
    end
})

rebirthTab:AddSwitch("Auto Rebirth (Infinite)", function(state)
    getgenv().autoRebirth = state
    if state then
        task.spawn(function()
            while getgenv().autoRebirth do
                pcall(function()
                    local remote = ReplicatedStorage.rEvents:FindFirstChild("rebirthRemote")
                    if remote then remote:InvokeServer("rebirthRequest") end
                end)
                task.wait(0.08)
            end
        end)
    end
end)

rebirthTab:AddSwitch("Auto Rebirth → Target", function(state)
    getgenv().autoRebirthTarget = state
    if state then
        task.spawn(function()
            while getgenv().autoRebirthTarget do
                pcall(function()
                    if LocalPlayer.leaderstats.Rebirths.Value >= targetRebirth then
                        getgenv().autoRebirthTarget = false
                        return
                    end
                    local remote = ReplicatedStorage.rEvents:FindFirstChild("rebirthRemote")
                    if remote then remote:InvokeServer("rebirthRequest") end
                end)
                task.wait(0.1)
            end
        end)
    end
end)

rebirthTab:AddSwitch("Auto Size 2", function(state)
    if state then
        getgenv().size2Loop = task.spawn(function()
            while true do
                pcall(function()
                    local remote = ReplicatedStorage.rEvents:FindFirstChild("changeSpeedSizeRemote")
                    if remote then remote:InvokeServer("changeSize", 2) end
                end)
                task.wait(0.06)
            end
        end)
    else
        if getgenv().size2Loop then task.cancel(getgenv().size2Loop) end
    end
end)

-- =============================================
--  Misc Tab
-- =============================================
MiscFolder:AddSwitch("Infinite Jump", function(state)
    getgenv().infJump = state
end)

UserInputService.JumpRequest:Connect(function()
    if getgenv().infJump and LocalPlayer.Character then
        LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

MiscFolder:AddSwitch("Remove Portals (Anti Portal)", function(state)
    if state then
        -- Initial cleanup
        for _, obj in pairs(game:GetDescendants()) do
            if obj.Name == "RobloxForwardPortals" then
                obj:Destroy()
            end
        end
        -- Prevent new ones
        getgenv().portalConn = game.DescendantAdded:Connect(function(obj)
            if obj.Name == "RobloxForwardPortals" then
                obj:Destroy()
            end
        end)
    else
        if getgenv().portalConn then
            getgenv().portalConn:Disconnect()
            getgenv().portalConn = nil
        end
    end
end)

-- =============================================
--  Better Farming Tab (example slot)
-- =============================================
betterFarming:AddSwitch("Fast Grind / Strength", function(state)
    -- Add your fast strength logic here if you have it
    print("Fast Grind:", state and "ON" or "OFF")
end)

print("Invincibles Priv Script loaded (cleaned version)")
