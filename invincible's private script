-- Stats: Player stats tracker
local function updatePlayerStats()
    local player = game.Players.LocalPlayer
    local leaderstats = player:FindFirstChild("leaderstats")
    local goodKarma = player:FindFirstChild("goodKarma")
    local evilKarma = player:FindFirstChild("evilKarma")
    local Durability = player:FindFirstChild("Durability")
    local Agility = player:FindFirstChild("Agility")
    local muscleKingTime = player:FindFirstChild("muscleKingTime")

    local sessionTime = os.time() - sessionStartTime
    TimerParagraph:SetContent(formatTime(sessionTime))
    if timerRunning then
        local elapsed = os.time() - startTime
        CustomTimerParagraph:SetContent(formatTime(elapsed))
    end

    -- Stats calculations
    local currentStrength = leaderstats and leaderstats:FindFirstChild("Strength") and leaderstats.Strength.Value or 0
    local currentRebirths = leaderstats and leaderstats:FindFirstChild("Rebirths") and leaderstats.Rebirths.Value or 0
    local currentKills = leaderstats and leaderstats:FindFirstChild("Kills") and leaderstats.Kills.Value or 0
    local currentBrawls = leaderstats and leaderstats:FindFirstChild("Brawls") and leaderstats.Brawls.Value or 0
    local currentGoodKarma = goodKarma and goodKarma.Value or 0
    local currentEvilKarma = evilKarma and evilKarma.Value or 0
    local currentDurability = Durability and Durability.Value or 0
    local currentAgility = Agility and Agility.Value or 0
    local currentMuscleKingTime = muscleKingTime and muscleKingTime.Value or 0

    -- Keep previous values and calculate gains
    -- (Implement your own tracking logic here, similar to previous code)

    -- Update UI with latest stats
    LeaderParagraph:SetContent(string.format("Strength: %s\nRebirths: %s\nKills: %s\nBrawls: %s", formatNumber(currentStrength), formatNumber(currentRebirths), formatNumber(currentKills), formatNumber(currentBrawls)))
    IntParagraph:SetContent(string.format("Good Karma: %s\nEvil Karma: %s\nDurability: %s\nAgility: %s\nMuscle King Time: %s", formatNumber(currentGoodKarma), formatNumber(currentEvilKarma), formatNumber(currentDurability), formatNumber(currentAgility), formatNumber(currentMuscleKingTime)))
end

RunService.RenderStepped:Connect(updatePlayerStats)

